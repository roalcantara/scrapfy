{
  "name": "scrapfy",
  "version": "0.0.0",
  "license": "MIT",
  "description": "A simple scrapper",
  "keywords": [
    "boilerplate",
    "typescript",
    "crawler",
    "nestjs",
    "scrap",
    "cli"
  ],
  "author": "@roalcantara",
  "url": "https://github.com/roalcantara/scrapfy/issues",
  "email": "rogerio.alcantara@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/roalcantara/scrapfy.git"
  },
  "engines": {
    "node": "10",
    "yarn": ">=1.9.0 <2.0.0"
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "setup": "yarn global add typescript && prettier && npx && jest",
    "clear": "rm -r ./dist && rm -r .tmp || true",
    "purge": "yarn clear && rm -rf node_modules",
    "reset": "yarn purge && yarn",
    "release": "npx semantic-release",
    "release:dry-run": "export $(cat .env | xargs) && npx semantic-release --dry-run --no-ci --debug"
  },
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "debug": "^4.1.1",
    "figlet": "^1.3.0",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "mz": "^2.7.0",
    "ora": "^4.0.4",
    "pretty-format": "^25.4.0",
    "puppeteer": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.1",
    "tty-table": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nrwl/eslint-plugin-nx": "9.2.3",
    "@nrwl/jest": "9.2.3",
    "@nrwl/node": "^9.2.3",
    "@nrwl/workspace": "9.2.3",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/chalk": "^2.2.0",
    "@types/cheerio": "^0.22.17",
    "@types/faker": "^4.1.11",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/mz": "^2.7.0",
    "@types/node": "~13.13.2",
    "@types/ora": "^3.2.0",
    "@types/puppeteer": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "factory-bot-ts": "^0.1.5",
    "faker": "^4.1.0",
    "html-loader-jest": "^0.2.1",
    "husky": "^4.2.5",
    "jest": "25.4.0",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "semantic-release": "^17.0.7",
    "ts-jest": "25.4.0",
    "ts-node": "~8.9.0",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npx pretty-quick --staged",
      "postcommit": "git update-index -g"
    }
  }
}
