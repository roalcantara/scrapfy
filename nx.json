{
  "npmScope": "scrapfy",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "util": {
      "tags": ["scope:shared"],
      "implicitDependencies": []
    },
    "meta": {
      "tags": ["scope:shared"],
      "implicitDependencies": []
    },
    "log": {
      "tags": ["scope:shared"],
      "implicitDependencies": []
    },
    "http": {
      "tags": ["scope:shared"],
      "implicitDependencies": []
    },
    "crawler": {
      "tags": ["scope:shared"],
      "implicitDependencies": []
    }
  }
}
